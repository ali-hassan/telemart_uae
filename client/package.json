    "@babel/runtime": "7.4.4",
    "axios": "^0.21.2",
    "babel-loader": "8.2.5",
    "check-node-version": "3.3.0",
    "classnames": "2.2.5",
    "css-loader": "3.0.0",
    "es6-shim": "0.35.6",
    "file-loader": "3.0.1",
    "i18n-js": "http://github.com/sharetribe/i18n-js/archive/interpolation-mode.tar.gz",
    "immutable": "3.8.1",
    "imports-loader": "0.6.5",
    "loader-utils": "0.2.16",
