    "@storybook/addons": "^4.1.18",
    "@storybook/react": "^4.1.18",
    "babel-eslint": "10.0.1",
    "babel-plugin-react-transform": "3.0.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "enzyme": "2.6.0",
    "eslint": "4.18.2",
    "eslint-plugin-babel": "4.1.2",
